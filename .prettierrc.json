{
  "printWidth": 128,
  "useTabs": false,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": false,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "ignore",
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
